import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int tickets = sc.nextInt();
        sc.nextLine();  
        String refreshment = sc.nextLine();
        String coupon = sc.nextLine();
        String type = sc.nextLine();
        if (tickets < 5 || tickets > 40) {
            System.out.println("Minimum of 5 and Maximum of 40 Tickets");
        } else {
            if (!type.equalsIgnoreCase("k") && !type.equalsIgnoreCase("q")) {
                System.out.println("Invalid Input");
            } else {
                double ticketCost = 0.0;
                if (type.equalsIgnoreCase("k")) {
                    ticketCost = 75.0;
                } else if (type.equalsIgnoreCase("q")) {
                    ticketCost = 150.0;
                }
                double totalCost = ticketCost * tickets;
                if (tickets > 20) {
                    totalCost = totalCost - (totalCost * 0.10);
                }
                if (coupon.equalsIgnoreCase("y")) {
                    totalCost = totalCost - (totalCost * 0.02);
                }
                if (refreshment.equalsIgnoreCase("y")){
                    totalCost = totalCost + (tickets * 50.0);
                }
                System.out.printf("Ticket cost:%.2f\n", totalCost);
            }
        }
    }

}
    
